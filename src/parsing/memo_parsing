	char **map = ft_parse_map(av[1]);
	int i = 0;
	while (map[i])
	{
		ft_putchar_fd('*', 2);
		ft_putstr_fd(map[i], 2);
		ft_putchar_fd('*', 2);
		ft_putchar_fd('\n', 2);
		i++;
	}
